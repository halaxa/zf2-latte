{
    "name": "halaxa/zf2-latte",
    "description": "Zf2 module for Latte templating engine integration",
    "license": "BSD-3-Clause",
    "keywords": [
        "zf2",
        "templates",
        "latte"
    ],
    "homepage": "http://doc.nette.org/en/2.1/templating",
    "require": {
        "php": ">=5.3.3",
        "zendframework/zendframework": "2.*",
        "latte/latte": "2.2.*@dev"
    },
    "require-dev": {
        "tracy/tracy": "dev-master",
        "nette/tester": "~1.1"
    },
    "autoload": {
        "psr-0": {
            "Zf2Latte": "src/"
        },
        "classmap": ["Module.php"]
    },
    "autoload-dev": {
        "psr-0": {
            "Application": "test/app/module/Application/src/"
        },
        "classmap": ["test/app/module/Application/Module.php"]
    }
}
